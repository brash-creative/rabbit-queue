{
    "name": "brash-creative/rabbit-queue",
    "description": "A basic package to publish and/or consume AMQP messages via RabbitMQ",
    "require": {
        "php": ">=7.0",
        "php-amqplib/php-amqplib": "~2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.4",
        "mockery/mockery": "~1.0"
    },
    "authors": [
        {
            "name": "paulcrashley",
            "email": "paulcrashley@gmail.com"
        }
    ],
    "options": {
        "http": {
            "proxy": ""
        }
    },
    "autoload": {
        "psr-4": {
            "Brash\\RabbitQueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brash\\RabbitQueue\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev"
}
